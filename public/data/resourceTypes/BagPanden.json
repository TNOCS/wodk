        "Adres": {
            "label": "Adres",
            "titel": "Adres",
            "type": "textarea",
            "expression": "properties.openbareruimtenaam + ' ' + properties.huisnummer + properties.huisletter||'' + properties.huisnummertoevoeging||'' + '\n' + properties.postcode + ' ' + properties.woonplaatsnaam"
        },
        "postcode": {
            "label": "postcode",
            "type": "text",
            "title": "Postcode",
            "visibleInCallOut": false,
            "canEdit": false,
            "isSearchable": false
