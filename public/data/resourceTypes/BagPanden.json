{
    "featureTypes": {
        "BagPanden": {
            "name": "BagPanden",
            "contourProperty": "contour",
            "style": {
                "fillColor": "#99ff99",
                "strokeColor": "#000000",
                "strokeWidth": 1,
                "iconWidth": 30,
                "iconHeight": 30,
                "iconUri": "images/homeicon{ster_gem}.png",
                "maxTitleResolution": -1
            },
            "propertyTypeKeys": "Adres;postcode;Name;woonplaatsnaam;gemeentenaam;wijknaam;wijkcode;buurtnaam;buurtcode;pandoppervlakte;pandhoogte;bouwjaar;woningen_in_pand;lift;ster_onb;ster_0;ster_1;ster_2;ster_3;ster_4;ster_5;ster_gem;woningtype;oppervlakteverblijfsobject;gebruiksdoelverblijfsobject;pandid;"
        }
    },
