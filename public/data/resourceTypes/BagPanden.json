        "ster_gem": {
            "label": "ster_gem",
            "type": "number",
            "title": "Gemiddelde ster",
            "expression": "(properties.ster_onb > 0) ? 6 : ((properties.ster_1 * 1 ||0 + properties.ster_2 * 2 ||0 + properties.ster_3 * 3 ||0 + properties.ster_4 * 4 ||0 + properties.ster_5 * 5 ||0)/properties.woningen_in_pand)",
            "visibleInCallOut": true,
            "isSearchable": false,
            "legend": {
                "id": "ster_gem",
                "visualAspect": "fillColor",
                "description": "Gemiddelde ster",
                "legendKind": "discrete",
                "legendEntries": [
                    {
                        "label": "Ster 0 - niet-aanpasbare woning",
                        "interval": {
                            "min": -1,
                            "max": 0
                        },
                        "color": "#ff0000"
                    },
                    {
                        "label": "Ster 1 - niet-aanpasbare woning",
                        "interval": {
                            "min": 0,
                            "max": 1
                        },
                        "color": "#ff9900"
                    },
                    {
                        "label": "Ster 2 - aanpasbare woning",
                        "interval": {
                            "min": 1,
                            "max": 2
                        },
                        "color": "#33ff33"
                    },
                    {
                        "label": "Ster 3 - aanpasbare woning",
                        "interval": {
                            "min": 2,
                            "max": 3
                        },
                        "color": "#00dd00"
                    },
                    {
                        "label": "Ster 4 - specifieke woning",
                        "interval": {
                            "min": 3,
                            "max": 4
                        },
                        "color": "#009922"
                    },
                    {
                        "label": "Ster 5 - specifieke woning",
                        "interval": {
                            "min": 4,
                            "max": 5
                        },
                        "color": "#007722"
                    }
                ]
            }
