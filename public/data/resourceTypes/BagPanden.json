{
    "featureTypes": {
        "BagPanden": {
            "name": "BagPanden",
            "contourProperty": "contour",
            "style": {
                "fillColor": "#9999ff",
                "fillOpacity": 0.85,
                "opacity": 0.85,
                "strokeColor": "#000000",
                "strokeWidth": 1,
                "strokeOpacity": 1,
                "iconWidth": 30,
                "iconHeight": 30,
                "cornerRadius": 6,
                "iconUri": "images/bag_{icon}.png",
                "maxTitleResolution": -1,
                "drawingMode": "Point"
            },
            "propertyTypeKeys": "Name;aanpasbaarheid;kostenindicatie;Adres;gemeentenaam;streetview;pandtype;bouwjaar;lift;pandoppervlakte;plint;pandhoogte;woningen_in_pand;niet_woningen_in_pand;gebruiksdoelverblijfsobject;pandid;kadaster;ster_0;ster_1;ster_2;ster_3;ster_4;ster_5;ster_onb;wijknaam;wijkcode;buurtnaam;buurtcode;ster_gem;oppervlakteverblijfsobject;icon;postcode;woonplaatsnaam;"
        }
    },
    "propertyTypeData": {
        "Adres": {
            "label": "Adres",
            "title": "Adres",
            "type": "textarea",
            "expression": "properties.openbareruimtenaam + ' ' + properties.huisnummer + (properties.huisletter||'') + (properties.huisnummertoevoeging||'') + '\n' + properties.postcode + ' ' + properties.woonplaatsnaam"
        },
        "kostenindicatie": {
            "label": "kostenindicatie",
            "title": "Kostenindicatie aanpassingen",
            "visibleInCallOut": true,
            "type": "textarea",
            "expression": "(properties.woningen_in_pand === 1 && !properties.niet_woningen_in_pand) ? 'traplift (nieuw): €3500 - €7500 \ntraplift (hergebruik): ca. 50% van nieuw' : null"
